<song createdIn="OpenLP 2.0.1" modifiedDate="2014-06-28T13:05:15" modifiedIn="atollib.py 2014.06.28 (openlyrics.py 0.2)" version="0.8" xmlns="http://openlyrics.info/namespace/2009/song">
  <properties>
    <titles>
      <title lang="hu">Dicsérd, dicsérd</title>
      <title lang="en">Praise Him</title>
    </titles>
    <authors>
      <author>Twila Paris</author>
      <author lang="hu" type="translation">Németh S. Judit</author>
    </authors>
    <songbooks>
      <songbook entry="64" name="Szent András Énekfüzet" />
    </songbooks>
    <copyright>© 1984 New Spring</copyright>
    <verseOrder>v1 c1</verseOrder>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Dicsérd, dicsérd, dicsérd az Urat,<br />dicsérd, dicsérd, mert Ő irgalmas!</lines>
    </verse>
    <verse name="c1">
      <lines>És Ő jó, hódolatra méltó,<br />ha Hozzá fordulsz, nem vet el,<br />erős karja felemel, halleluja!</lines>
    </verse>
  </lyrics>
</song>
