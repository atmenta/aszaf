  <properties>
    <titles>
      <title>Uram, odanyújtom</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="80"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Uram, odanyújtom a lázas kezem!<br/>Te tudod, hogy mitől beteg az én szívem!</lines>
    </verse>
    <verse name="c1">
      <lines>Érints meg, érints meg!<br/>Gyógyíts meg, gyógyíts meg!<br/>Tisztíts meg, tisztíts meg!</lines>
    </verse>
  </lyrics>
</song>
