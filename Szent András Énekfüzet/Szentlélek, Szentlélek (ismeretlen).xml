      <title>Szentlélek, Szentlélek</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="222"/>
    </songbooks>
