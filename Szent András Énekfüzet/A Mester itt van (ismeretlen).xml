<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-05T22:54:52">
  <properties>
    <titles>
      <title>A Mester itt van</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="54"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>A Mester itt van, ma is gyógyít,<br/>Jézus gyógyít és szabaddá tesz.</lines>
    </verse>
    <verse name="v2">
      <lines>Ne vidd tovább a terhedet,<br/>Ő letörli minden könnyedet,<br/>gyászod nagy örömre fordul!</lines>
    </verse>
  </lyrics>
</song>
