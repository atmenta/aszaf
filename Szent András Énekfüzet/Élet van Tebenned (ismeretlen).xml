<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-06T09:57:12">
  <properties>
    <titles>
      <title>Élet van Tebenned</title>
    </titles>
    <verseOrder>c1 v1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="3"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Élet van Tebenned, erő van Tebenned,<br/>remény van Tebenned, ó, Jézus!</lines>
    </verse>
    <verse name="v1">
      <lines>Így dicsérlek életemmel,<br/>és áldalak minden erőmmel,<br/>az életemmel, minden erőmmel,<br/>csak Benned van reménységem.</lines>
    </verse>
  </lyrics>
</song>
