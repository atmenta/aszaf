  <properties>
    <titles>
      <title>Ünnepeld Jézust</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="97"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ünnepeld Jézust, ünnepeld!<br/>Ünnepeld Jézust, ünnepeld!<br/>Feltámadt, feltámadt,<br/>és él örökké!<br/>Feltámadt, feltámadt!<br/>Gyere és ünnepeld! A mi Urunk feltámadt és él!</lines>
    </verse>
  </lyrics>
</song>
