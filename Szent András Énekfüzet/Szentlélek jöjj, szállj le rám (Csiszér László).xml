    <titles>
      <title lang="hu">Szentlélek jöjj, szállj le rám</title>
      <title lang="hu">Szerelmem Szentlélek</title>
    </titles>
    <authors>
      <author>Csiszér László</author>
    </authors>
    <songbooks>
      <songbook entry="314" name="Szent András Énekfüzet" />
    </songbooks>
    <verseOrder>v1 c1</verseOrder>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Szentlélek, jöjj, szállj le rám<br />és töltsél el egészen!</lines>
    </verse>
    <verse name="c1">
      <lines>Mert csak benned van erő,<br />és benned van élet,<br />szerelmem, drága Szentlélek!</lines>
    </verse>
  </lyrics>
