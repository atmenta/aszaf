<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-06T23:34:28">
  <properties>
    <titles>
      <title>Ünneplek, éneklek</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="134"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ünneplek, éneklek Istenem, az Úr előtt,<br/>háladalt zengek néki most és örökké!</lines>
    </verse>
    <verse name="c1">
      <lines>Magasztalom, dicsérem én,<br/>mert nagy csodákat vitt ő végbe!</lines>
    </verse>
  </lyrics>
</song>
