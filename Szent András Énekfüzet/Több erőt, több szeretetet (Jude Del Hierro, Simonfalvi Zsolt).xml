  <properties>
    <titles>
      <title lang="hu">Több erőt, több szeretetet</title>
      <title lang="en">More Love More Power</title>
    </titles>
    <authors>
      <author>Jude Del Hierro</author>
      <author lang="hu" type="translation">Simonfalvi Zsolt</author>
    </authors>
    <songbooks>
      <songbook entry="96" name="Szent András Énekfüzet" />
    </songbooks>
