  <properties>
    <titles>
      <title lang="hu">Szent vagy… Jahve Sabaoth</title>
      <title lang="es">Santo Sabaoth</title>
    </titles>
    <authors>
      <author>Kiko Argüello</author>
      <author lang="hu" type="translation">Csiszér László</author>
    </authors>
    <songbooks>
      <songbook entry="41" name="Énekfüzet" />
    </songbooks>
    <verseOrder>c1 v1 c1 v2 c1</verseOrder>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Szent vagy, szent vagy, szent vagy...<br />Jahve Sabaoth!</lines>
    </verse>
    <verse name="v1">
      <lines>Az ég és a föld zengi áldott Nevedet.<br />Hozsanna a magasságban, hozsanna!</lines>
    </verse>
    <verse name="v2">
      <lines>Ó, áldott, aki jön az Úr nevében.<br />Hozsanna a magasságban, hozsanna!</lines>
    </verse>
  </lyrics>
</song>