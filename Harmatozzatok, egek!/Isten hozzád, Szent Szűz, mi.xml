  <properties>
    <titles>
      <title>Isten hozzád, Szent Szűz, mi</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Harmatozzatok, egek!" entry="237"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Isten hozzád, Szent Szűz, mi már elmegyünk!<br/>Minden lépésünket neked szenteljük.<br/>Halálunknak végóráján jöjj el mihozzánk,<br/>Szent Fiaddal, Úr Jézussal légy kegyes hozzánk!</lines>
    </verse>
  </lyrics>
</song>
