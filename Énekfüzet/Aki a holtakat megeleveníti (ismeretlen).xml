<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-05T23:26:14">
  <properties>
    <titles>
      <title>Aki a holtakat megeleveníti</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="48"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Aki a holtakat megeleveníti,<br/>és azokat, amelyek nincsenek,<br/>előszólítja, mint meglevőket.</lines>
    </verse>
    <verse name="c1">
      <lines>Jézus, Te vagy minden örömöm forrása!<br/>Jézus, Te vagy, Te vagy az élet!</lines>
    </verse>
  </lyrics>
</song>
