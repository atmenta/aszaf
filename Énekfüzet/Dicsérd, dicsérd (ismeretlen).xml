<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-06T09:39:06">
  <properties>
    <titles>
      <title>Dicsérd, dicsérd</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="64"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Dicsérd, dicsérd, dicsérd az Urat,<br/>dicsérd, dicsérd, mert Ő irgalmas!</lines>
    </verse>
    <verse name="c1">
      <lines>És Ő jó, hódolatra méltó,<br/>ha Hozzá fordulsz, nem vet el,<br/>erős karja felemel, halleluja!</lines>
    </verse>
  </lyrics>
</song>
