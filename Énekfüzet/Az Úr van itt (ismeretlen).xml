<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-06T00:10:44">
  <properties>
    <titles>
      <title>Az Úr van itt</title>
    </titles>
    <verseOrder>v1 p1 c1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="59"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Az Úr van itt,<br/>a te Istened,<br/>Ő vár rád,<br/>Ő vár rád,</lines>
    </verse>
    <verse name="p1">
      <lines>hogy elmondja, mennyire szeret,<br/>a legdrágábbat adta érted!</lines>
    </verse>
    <verse name="c1">
      <lines>Az Ő Fiát,<br/>Aki most is él,<br/>s uralkodik mindörökké.</lines>
    </verse>
  </lyrics>
</song>
