  <properties>
    <titles>
      <title>Hazamegyek</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>Csiszér László</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="51"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Hazamegyek, ez a világ nem a hazám,<br/>tudom, hogy Ő, Jézus Király, vár énreám!</lines>
    </verse>
    <verse name="c1">
      <lines>Jöjj el, jöjj el, hát! Jöjj el, Jézus Király!</lines>
    </verse>
  </lyrics>
