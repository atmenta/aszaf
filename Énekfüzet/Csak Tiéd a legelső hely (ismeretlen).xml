  <properties>
    <titles>
      <title>Csak Tiéd a legelső hely</title>
    </titles>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="197"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Csak Tiéd a legelső hely,<br/>mert Te, Te vagy a hű Főpap,<br/>rangod magasabb minden rangnál,<br/>trónodhoz jövünk, imádunk, szent Urunk!</lines>
    </verse>
  </lyrics>
</song>
