<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.0.1" modifiedIn="OpenLP 2.0.1" modifiedDate="2013-02-06T14:19:30">
  <properties>
    <titles>
      <title>Jöjj a Szentek Szentjébe</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author>ismeretlen</author>
    </authors>
    <songbooks>
      <songbook name="Énekfüzet" entry="133"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jöjj a Szentek Szentjébe, jöjj az Ő jelenlétébe,<br/>a Bárány, Jézus vérén át, imádd Istent a trónjánál!</lines>
    </verse>
    <verse name="c1">
      <lines>Szent kezeket emelj fel,<br/>mert szent az Isten, igen, szent!</lines>
    </verse>
  </lyrics>
</song>
