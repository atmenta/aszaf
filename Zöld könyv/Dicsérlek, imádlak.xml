  <properties>
    <titles>
      <title>Dicsérlek, imádlak</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="ZöldKönyv" entry="443"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Dicsérlek, imádlak,<br/>életemnek célja csak Te<br/>vagy, hogy magasztaljalak,<br/>és áldjam Szent Neved.<br/>Ó Uram, szeretlek Téged!<br/>Vágyom hallani beszédedet,<br/>és Veled járni mindörökké!</lines>
    </verse>
  </lyrics>
</song>
