    <titles>
      <title>Jöjj, hívunk, jöjj</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="ZöldKönyv" entry="286"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jöjj, hívunk, jöjj,<br/>Szentlélek, a szívünkbe áradj!<br/>Jöjj, hívunk jöjj,<br/>Mint folyó, amely túlárad!<br/>Úgy várunk, és kiáltunk.<br/>Hívunk, jöjj…<br/>Hívunk jöjj, áradj szét…</lines>
    </verse>
  </lyrics>
