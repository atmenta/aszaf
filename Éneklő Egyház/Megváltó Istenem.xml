  <properties>
    <titles>
      <title>Megváltó Istenem</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author></author>
    </authors>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="210"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Megváltó Istenem,<br/>könyörülj lelkemen,<br/>szólj hozzám, én Uram,<br/>ajkadon béke van,<br/>alle-, alleluja.</lines>
    </verse>
    <verse name="v2">
      <lines>Könnyes az ételem,<br/>italom gyötrelem,<br/>benned megújhodom,<br/>te vagy az irgalom,<br/>alle-, alleluja.</lines>
    </verse>
  </lyrics>
</song>
